  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Launch Program",
    "stopOnEntry": true,
    "cwd": "${workspaceFolder}",
    "program": "${workspaceFolder}/node_modules/.bin/protractor",
    "args": ["${workspaceFolder}/e2e/protractor.conf.js"],
    "protocol": "inspector"
  }]
}
